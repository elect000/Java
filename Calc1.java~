
public class Calc1 {
	public static void main(String[] args) {
		System.out.println(" "+(2+3)+" ");
		System.out.println(" "+(2-3)+" ");
		System.out.println(" "+(2*3)+" ");
		System.out.println(" "+(2/3)+" ");
		System.out.println(" "+(2%3)+" ");
	}
    
}

public class Calc2 {
	public static void main(String[] args) {
		System.out.println("201611350"+(201611350/7)+" ");
		System.out.println("201611350"+(201611350%7)+" ");
	}
    
}
